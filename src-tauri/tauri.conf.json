{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "물리치료학과 대시보드",
    "version": "1.0.5",
    "identifier": "com.koreapt.dashboard",
    "build": {
        "beforeDevCommand": "npm run dev",
        "devUrl": "http://localhost:8080",
        "beforeBuildCommand": "npm run build",
        "frontendDist": "../dist"
    },
    "app": {
        "withGlobalTauri": false,
        "windows": [
            {
                "title": "물리치료학과 대시보드",
                "width": 1400,
                "height": 900,
                "minWidth": 800,
                "minHeight": 600,
                "resizable": true,
                "fullscreen": false,
                "visible": true
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "macOS": {
            "minimumSystemVersion": "10.13"
        },
        "createUpdaterArtifacts": true
    },
    "plugins": {
        "updater": {
            "endpoints": [
                "https://github.com/skyman200/dphysio/releases/latest/download/latest.json"
            ],
            "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDBBRjYzMDgzQzc2RkJCQTgKUldTb3UyL0hnekQyQ3NmNnJWMW9YaUg2WWdHZmpqWGU2QjdXNlB1REt0Y1huVFZRQUdCVDVTSXkK"
        }
    }
}